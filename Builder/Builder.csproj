<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Template\ControllerTemplate.txt" />
    <None Remove="Template\DtoTemplate.txt" />
    <None Remove="Template\ModelTemplate.txt" />
    <None Remove="Template\ServiceTemplate.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Template\ControllerTemplate.txt" />
    <EmbeddedResource Include="Template\DtoTemplate.txt" />
    <EmbeddedResource Include="Template\ModelTemplate.txt" />
    <EmbeddedResource Include="Template\ServiceTemplate.txt" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

</Project>
